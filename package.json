{
  "name": "express-graphql",
  "version": "0.12.0",
  "description": "Production ready GraphQL HTTP middleware.",
  "license": "MIT",
  "private": true,
  "main": "index.js",
  "types": "index.d.ts",
  "typesVersions": {
    ">=4.1.0": {
      "*": [
        "*"
      ]
    },
    "*": {
      "*": [
        "NotSupportedTSVersion.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "homepage": "https://github.com/graphql/express-graphql",
  "bugs": {
    "url": "https://github.com/graphql/express-graphql/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/graphql/express-graphql.git"
  },
  "keywords": [
    "express",
    "restify",
    "connect",
    "http",
    "graphql",
    "middleware",
    "api"
  ],
  "engines": {
    "node": ">= 10.x"
  },
  "scripts": {
    "build:npm": "node resources/build-npm.js",
    "changelog": "node resources/gen-changelog.js",
    "check": "tsc --noEmit",
    "check:integrations": "mocha --full-trace integrationTests/*-test.js",
    "check:spelling": "cspell '**/*'",
    "lint": "eslint .",
    "prettier": "prettier --write --list-different .",
    "prettier:check": "prettier --check .",
    "preversion": ". ./resources/checkgit.sh && npm ci",
    "start": "node -r ./resources/register.js examples/index.ts",
    "start:subscription": "node -r ./resources/register.js examples/index_subscription.ts",
    "start:subscription_legacy": "node -r ./resources/register.js examples/index_subscription_legacy.ts",
    "test": "npm run lint && npm run check && npm run testonly:cover && npm run prettier:check && npm run check:spelling && npm run build:npm && npm run check:integrations",
    "testonly": "node_modules/.bin/mocha src/**/__tests__/**/*.ts --globals Generator",
    "testonly:cover": "c8 --100 npm run testonly",
    "version": "npm test"
  },
  "dependencies": {
    "accepts": "^1.3.7",
    "c8": "^7.10.0",
    "content-type": "^1.0.4",
    "eslint-plugin-mocha": "^9.0.0",
    "get-stream": "^6.0.1",
    "http-errors": "1.8.1"
  },
  "devDependencies": {
    "@graphiql/toolkit": "^0.4.2",
    "@types/accepts": "1.3.5",
    "@types/body-parser": "1.19.2",
    "@types/chai": "4.2.22",
    "@types/connect": "3.4.35",
    "@types/content-type": "1.1.5",
    "@types/express": "4.17.13",
    "@types/http-errors": "1.8.1",
    "@types/mocha": "9.0.0",
    "@types/multer": "1.4.7",
    "@types/node": "16.11.11",
    "@types/restify": "8.5.4",
    "@types/sinon": "10.0.6",
    "@types/supertest": "2.0.11",
    "@types/ws": "8.2.1",
    "@typescript-eslint/eslint-plugin": "5.5.0",
    "@typescript-eslint/parser": "5.5.0",
    "body-parser": "1.19.0",
    "chai": "4.3.4",
    "connect": "3.7.0",
    "cspell": "5.13.1",
    "downlevel-dts": "0.7.0",
    "eslint": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-internal-rules": "file:./resources/eslint-internal-rules",
    "eslint-plugin-istanbul": "0.1.2",
    "eslint-plugin-node": "11.1.0",
    "express": "4.17.1",
    "graphiql": "^1.5.7",
    "graphiql-subscriptions-fetcher": "0.0.2",
    "graphql": "16.0.1",
    "graphql-ws": "5.5.5",
    "mocha": "9.1.3",
    "multer": "1.4.3",
    "nyc": "15.1.0",
    "prettier": "2.5.0",
    "promise-polyfill": "8.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "restify": "8.6.0",
    "sinon": "12.0.1",
    "subscriptions-transport-ws": "0.11.0",
    "supertest": "6.1.6",
    "ts-node": "10.4.0",
    "typescript": "4.5.2",
    "unfetch": "4.2.0",
    "ws": "8.3.0"
  },
  "peerDependencies": {
    "graphql": "^15.5.0 || ^16.0.0"
  }
}
